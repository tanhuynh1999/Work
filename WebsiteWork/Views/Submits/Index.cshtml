@model IEnumerable<WebsiteWork.Models.Submit>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.user_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.submit_datecreated)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cv.cv_fullname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.User.user_login)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Work.work_name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.user_id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.submit_datecreated)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cv.cv_fullname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.User.user_login)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Work.work_name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.submit_id }) |
            @Html.ActionLink("Details", "Details", new { id=item.submit_id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.submit_id })
        </td>
    </tr>
}

</table>
